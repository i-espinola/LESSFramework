{
	"env": {
		"development": {
			"plugins": [
				[
					"babel-plugin-styled-components",
					{
						"ssr": true,
						"displayName": true,
						"preprocess": false
					}
				]
			],
			"presets": [
				"next/babel"
			]
		},
		"production": {
			"plugins": [
				[
					"babel-plugin-styled-components",
					{
						"ssr": true,
						"displayName": true,
						"preprocess": false
					}
				]
			],
			"presets": [
				"next/babel"
			]
		}
	},
	"plugins": [
		[
			"babel-plugin-styled-components",
			{
				"ssr": true,
				"displayName": true,
				"preprocess": false
			}
		],
		[
			"module-resolver",
			{
				"root": [
					"."
				],
				"alias": {
					"@components": "./src/components",
					"@contexts": "./src/contexts",
					"@helpers": "./src/helpers",
					"@hooks": "./src/hooks",
					"@layouts": "./src/layouts",
					"@pages": "./src/pages",
					"@services": "./src/services",
					"@tests": "./src/tests",
					"@theme": "./src/theme",
					"@utils": "./src/utils",
					"@screens": "./src/screens",
					"@svg": "./src/assets/svg/*"
				}
			}
		]
	]
}
